import { Schema, Record} from 'js-data';

export const <%= className %>Schema = new Schema ({
  title: '<%= className %>', 
  description: 'Schema for <%= className %> records', 
  type: 'object',
  properties: {
    <% Object.entries(properties).forEach(([key, val]) => { -%>
      <%= key %>: { type: '<%= val.tsType %>' },
    <% }) -%>
    }
});

export interface I<%= className %> extends Record {
  <% Object.entries(properties).forEach(([key, val]) => { -%>
    <%= key %>: { type: '<%= val.tsType %>' },
  <% }) -%>
}

